<div class="col-lg-12" id="list-area">
  <div class="col-lg-6" id="unselected-area">
    <h4>Unselected videos</h4>
    <ul class="videos-list" id="unselected-videos">
      <%= render partial: 'video', collection: @videos %>
    </ul>
  </div>

  <div class="col-lg-6" id="selected-area">
    <h4>Selected videos</h4>
    <ul class="videos-list" id="selected-videos">
    </ul>
  </div>
</div>
